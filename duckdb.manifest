[loader]
entrypoint = "file:/usr/lib/x86_64-linux-gnu/gramine/libsysdb.so"
log_level = "error"
insecure__use_cmdline_argv = true

[loader.env]
LD_LIBRARY_PATH = "/lib:/lib/x86_64-linux-gnu"
PATH = "/home/sdp/DuckDB-SGX"
HOME = "/"

[libos]
entrypoint = "/home/sdp/DuckDB-SGX/duckdb/build/release/duckdb"

[[fs.mounts]]
path = "/lib"
uri = "file:/usr/lib/x86_64-linux-gnu/gramine/runtime/glibc"

[[fs.mounts]]
path = "/lib/x86_64-linux-gnu"
uri = "file:/lib/x86_64-linux-gnu"

[[fs.mounts]]
path = "/home/sdp/DuckDB-SGX/duckdb/build/release/duckdb"
uri = "file:/home/sdp/DuckDB-SGX/duckdb/build/release/duckdb"

[[fs.mounts]]
path = "/home/sdp/DuckDB-SGX/duckdb/build/release/benchmark/benchmark_runner"
uri = "file:/home/sdp/DuckDB-SGX/duckdb/build/release/benchmark/benchmark_runner"

[fs.insecure__keys]
default = "ffeeddccbbaa99887766554433221100"

[sgx]
debug = true
edmm_enable = false
enclave_size = "8G"
max_threads = 16
preheat_enclave = true
trusted_files = [
    { uri = "file:/usr/lib/x86_64-linux-gnu/gramine/libsysdb.so" },
    { uri = "file:/home/sdp/DuckDB-SGX/duckdb/build/release/duckdb" },
    { uri = "file:/usr/lib/x86_64-linux-gnu/gramine/runtime/glibc/" },
    { uri = "file:/lib/x86_64-linux-gnu/" },
    { uri = "file:/home/sdp/DuckDB-SGX/scripts/" },
]
allowed_files = [
    "file:/home/sdp/DuckDB-SGX/benchmarks/",
]
isvprodid = 0
isvsvn = 0
remote_attestation = "none"
enable_stats = false
use_exinfo = false

[sgx.profile]
mode = "ocall_outer"
with_stack = 1

[sgx.cpu_features]
avx = "unspecified"
avx512 = "unspecified"
amx = "unspecified"
mpx = "disabled"
pkru = "disabled"
